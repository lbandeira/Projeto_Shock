<script>
  (function() {

    var btn = document.querySelector('#see-more , #see-more *');
    var searchField = document.querySelector('input');

    searchText = searchField ? searchField.value : '';
    
    btn.addEventListener("click", function(){
       console.log("Clicou");

       window.dataLayer.push({
        event: 'autocompleSearch',
        customSearchInput: searchText
      });
    })
    searchField.addEventListener("keyup", function(e){
      if (e.keyCode === 13) {
        console.log("Enter"); 

        window.dataLayer.push({
        event: 'autocompleteSearch',
        customSearchInput: searchText
      });
      }
    })
    
  })();
</script>